//# print-bytecode --input=module
// Moving a type defined as part of the same module is valid.
module 0x2d6.M {
    struct T has key { b: bool }

    public new(): Self.T {
    label b0:
        return T { b: true };
    }

    f(account: signer) {
    label b0:
        move_to<T>(&account, Self.new());
        return;
    }
}

