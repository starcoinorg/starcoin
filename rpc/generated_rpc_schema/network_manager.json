{
  "openrpc": "1.2.6",
  "info": {
    "title": "",
    "version": ""
  },
  "methods": [
    {
      "name": "network_manager.state",
      "params": [],
      "result": {
        "name": "NetworkState",
        "schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "title": "NetworkState",
          "description": "Returns general information about the networking.\n\nMeant for general diagnostic purposes.\n\n**Warning**: This API is not stable.",
          "type": "object",
          "required": [
            "connectedPeers",
            "externalAddresses",
            "listenedAddresses",
            "notConnectedPeers",
            "peerId",
            "peerset"
          ],
          "properties": {
            "connectedPeers": {
              "description": "List of node we're connected to.",
              "type": "object",
              "additionalProperties": {
                "description": "Part of the `NetworkState` struct. Unstable.",
                "type": "object",
                "required": [
                  "endpoint",
                  "knownAddresses"
                ],
                "properties": {
                  "endpoint": {
                    "description": "How we are connected to the node.",
                    "oneOf": [
                      {
                        "description": "We are dialing the given address.",
                        "type": "object",
                        "required": [
                          "dialing"
                        ],
                        "properties": {
                          "dialing": {
                            "type": "string",
                            "format": "Multiaddr"
                          }
                        },
                        "additionalProperties": false
                      },
                      {
                        "description": "We are listening.",
                        "type": "object",
                        "required": [
                          "listening"
                        ],
                        "properties": {
                          "listening": {
                            "type": "object",
                            "required": [
                              "local_addr",
                              "send_back_addr"
                            ],
                            "properties": {
                              "local_addr": {
                                "description": "Local address of the connection.",
                                "type": "string",
                                "format": "Multiaddr"
                              },
                              "send_back_addr": {
                                "description": "Address data is sent back to.",
                                "type": "string",
                                "format": "Multiaddr"
                              }
                            }
                          }
                        },
                        "additionalProperties": false
                      }
                    ]
                  },
                  "knownAddresses": {
                    "description": "List of addresses known for this node.",
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "Multiaddr"
                    },
                    "uniqueItems": true
                  },
                  "latestPingTime": {
                    "description": "Latest ping duration with this node.",
                    "type": [
                      "object",
                      "null"
                    ],
                    "required": [
                      "nanos",
                      "secs"
                    ],
                    "properties": {
                      "nanos": {
                        "type": "integer",
                        "format": "uint32",
                        "minimum": 0.0
                      },
                      "secs": {
                        "type": "integer",
                        "format": "uint64",
                        "minimum": 0.0
                      }
                    }
                  },
                  "versionString": {
                    "description": "Node information, as provided by the node itself. Can be empty if not known yet.",
                    "type": [
                      "string",
                      "null"
                    ]
                  }
                }
              }
            },
            "externalAddresses": {
              "description": "List of addresses the node knows it can be reached as.",
              "type": "array",
              "items": {
                "type": "string",
                "format": "Multiaddr"
              },
              "uniqueItems": true
            },
            "listenedAddresses": {
              "description": "List of addresses the node is currently listening on.",
              "type": "array",
              "items": {
                "type": "string",
                "format": "Multiaddr"
              },
              "uniqueItems": true
            },
            "notConnectedPeers": {
              "description": "List of node that we know of but that we're not connected to.",
              "type": "object",
              "additionalProperties": {
                "description": "Part of the `NetworkState` struct. Unstable.",
                "type": "object",
                "required": [
                  "knownAddresses"
                ],
                "properties": {
                  "knownAddresses": {
                    "description": "List of addresses known for this node.",
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "Multiaddr"
                    },
                    "uniqueItems": true
                  },
                  "latestPingTime": {
                    "description": "Latest ping duration with this node, if we were ever connected to this node.",
                    "type": [
                      "object",
                      "null"
                    ],
                    "required": [
                      "nanos",
                      "secs"
                    ],
                    "properties": {
                      "nanos": {
                        "type": "integer",
                        "format": "uint32",
                        "minimum": 0.0
                      },
                      "secs": {
                        "type": "integer",
                        "format": "uint64",
                        "minimum": 0.0
                      }
                    }
                  },
                  "versionString": {
                    "description": "Node information, as provided by the node itself, if we were ever connected to this node.",
                    "type": [
                      "string",
                      "null"
                    ]
                  }
                }
              }
            },
            "peerId": {
              "description": "PeerId of the local node.",
              "type": "string"
            },
            "peerset": {
              "description": "State of the peerset manager."
            }
          }
        }
      }
    },
    {
      "name": "network_manager.known_peers",
      "params": [],
      "result": {
        "name": "Vec < PeerId >",
        "schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "title": "Array_of_PeerId",
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    {
      "name": "network_manager.get_address",
      "params": [
        {
          "name": "peer_id",
          "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
