            "type": "string"
          }
        }
      ],
      "result": {
        "name": "Vec < Multiaddr >",
        "schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "title": "Array_of_Multiaddr",
          "type": "array",
          "items": {
            "type": "string",
            "format": "Multiaddr"
          }
        }
      }
    },
    {
      "name": "network_manager.add_peer",
      "params": [
        {
          "name": "peer",
          "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "title": "String",
            "type": "string"
          }
        }
      ],
      "result": {
        "name": "()",
        "schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "title": "Null",
          "type": "null"
        }
      }
    },
    {
      "name": "network_manager.call",
      "params": [
        {
          "name": "peer_id",
          "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "title": "String",
            "type": "string"
          }
        },
        {
          "name": "rpc_method",
          "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "title": "String",
            "type": "string"
          }
        },
        {
          "name": "message",
          "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "title": "alloc::vec::Vec<u8>",
            "type": "string"
          }
        }
      ],
      "result": {
        "name": "StrView < Vec < u8 > >",
        "schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "title": "alloc::vec::Vec<u8>",
          "type": "string"
        }
      }
    },
    {
      "name": "network_manager.set_peer_reput",
      "params": [
        {
          "name": "peer_id",
          "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "title": "String",
            "type": "string"
          }
        },
        {
          "name": "reputation",
          "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "title": "int32",
            "type": "integer",
            "format": "int32"
          }
        }
      ],
      "result": {
        "name": "()",
        "schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "title": "Null",
          "type": "null"
        }
      }
    },
    {
      "name": "network_manager.ban_peer",
      "params": [
        {
          "name": "peer_id",
          "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "title": "String",
            "type": "string"
          }
        },
        {
          "name": "ban",
          "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "title": "Boolean",
            "type": "boolean"
          }
        }
      ],
      "result": {
        "name": "()",
        "schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "title": "Null",
          "type": "null"
        }
      }
    }
  ]
}