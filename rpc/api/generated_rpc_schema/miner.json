      "params": [
        {
          "name": "minting_blob",
          "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "title": "String",
            "type": "string"
          }
        },
        {
          "name": "nonce",
          "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "title": "uint32",
            "type": "integer",
            "format": "uint32",
            "minimum": 0.0
          }
        },
        {
          "name": "extra",
          "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "title": "String",
            "type": "string"
          }
        }
      ],
      "result": {
        "name": "MintedBlockView",
        "schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "title": "MintedBlockView",
