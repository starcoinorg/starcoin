                },
                "job_id": {
                  "type": "string"
                },
                "worker_id": {
                  "type": "string"
                }
              }
            },
            "minting_blob": {
              "type": "string"
            },
            "parent_hash": {
              "type": "string",
              "format": "HashValue"
            },
            "strategy": {
              "oneOf": [
                {
                  "type": "object",
                  "required": [
                    "type"
                  ],
                  "properties": {
                    "type": {
                      "type": "string",
                      "enum": [
                        "Dummy"
                      ]
                    }
                  }
                },
                {
                  "type": "object",
                  "required": [
                    "type"
                  ],
                  "properties": {
                    "type": {
                      "type": "string",
                      "enum": [
                        "Argon"
                      ]
                    }
                  }
                },
                {
                  "type": "object",
                  "required": [
                    "type"
                  ],
                  "properties": {
                    "type": {
                      "type": "string",
                      "enum": [
                        "Keccak"
                      ]
                    }
                  }
                },
                {
                  "type": "object",
                  "required": [
                    "type"
                  ],
                  "properties": {
                    "type": {
                      "type": "string",
                      "enum": [
                        "CryptoNight"
                      ]
                    }
                  }
                }
              ]
            }
