          "required": [
            "block_hash"
          ],
          "properties": {
            "block_hash": {
              "type": "string",
              "format": "HashValue"
            }
          }
        }
      }
    },
    {
      "name": "mining.get_job",
      "params": [],
      "result": {
        "name": "Option < MintBlockEvent >",
        "schema": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "title": "Nullable_MintBlockEvent",
          "type": [
            "object",
            "null"
          ],
          "required": [
            "block_number",
            "difficulty",
            "minting_blob",
            "parent_hash",
            "strategy"
          ],
          "properties": {
            "block_number": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "difficulty": {
              "type": "string"
            },
            "extra": {
              "type": [
                "object",
                "null"
              ],
              "required": [
                "extra",
                "job_id",
                "worker_id"
              ],
              "properties": {
                "extra": {
                  "description": "Type for block header extra",
